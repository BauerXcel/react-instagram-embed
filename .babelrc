{
  "presets": ["es2017", "es2016", ["es2015", { "modules": false }], "react"],
  "plugins": ["transform-object-rest-spread", "transform-flow-strip-types", "transform-class-properties"],
  "env": {
    "commonjs": {
      "presets": ["es2015"],
      "plugins": ["transform-flow-strip-types", "transform-class-properties"]
    },
    "es": {},
    "development": {
      "plugins": ["react-hot-loader/babel"]
    },
    "test": {
      "presets": ["es2015"],
      "plugins": ["transform-flow-strip-types", "transform-class-properties"]
    }
  }
}
