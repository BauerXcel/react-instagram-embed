{
  "env": {
    "commonjs": {
      "presets": ["es2017", "es2016", "es2015", "react"],
      "plugins": ["transform-object-rest-spread", "transform-flow-strip-types", "transform-class-properties"],
    },
    "es": {
      "presets": ["es2017", "es2016", ["es2015", { "modules": false }], "react"],
      "plugins": ["transform-object-rest-spread", "transform-flow-strip-types", "transform-class-properties"],
    },
    "development": {
      "presets": ["es2017", "es2016", ["es2015", { "modules": false }], "react"],
      "plugins": ["transform-object-rest-spread", "transform-flow-strip-types", "transform-class-properties", "react-hot-loader/babel"]
    },
    "test": {
      "presets": ["es2017", "es2016", "es2015", "react"],
      "plugins": ["transform-object-rest-spread", "transform-flow-strip-types", "transform-class-properties"]
    }
  }
}
